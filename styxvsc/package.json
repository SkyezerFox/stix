{
    "name": "styxvsc",
    "displayName": "styxvsc",
    "description": "Provides language support for the styx language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "styx",
                "aliases": [
                    "Styx",
                    "styxlang"
                ],
                "extensions": [
                    ".stx",
                    ".styx"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "styx",
                "scopeName": "source.styx",
                "path": "./syntaxes/styx.tmLanguage.json"
            }
        ]
    }
}
